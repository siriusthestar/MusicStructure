<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.grubelic.nevena.musicstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/category_search" />
        <activity
            android:name=".PlaylistActivity"
            android:label="@string/category_playlist" />
        <activity
            android:name=".AlbumsActivity"
            android:label="@string/category_albums" />
        <activity
            android:name=".ArtistsActivity"
            android:label="@string/category_artists" />
        <activity
            android:name=".StoreActivity"
            android:label="@string/category_store" />
        <activity android:name=".PlayingActivity"
            android:label="@string/category_playing" />
    </application>

</manifest>